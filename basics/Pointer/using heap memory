#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main()
{
   int *p;
   p = (int *)malloc(6*sizeof(int));// used to access in heap memory 
   
   p[0]=31;p[1]=24;p[2]=8;p[3]=41;p[4]=6;p[5]=12;
   
   for(int i=0;i < 5 ; i++){
       
       cout<<p[i]<<endl;
   }
    return 0;
}
