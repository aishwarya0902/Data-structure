#include <iostream>
#include <stdio.h>

using namespace std;

int main()
{
   int A[5] = {1,2,3,4,5};
   int *p;
   p = A; // in array we no need to declare the & this will indicate the address
   for(int i=0;i < 5 ; i++){
       cout<<A[i]<<endl;
       cout<<p[i]<<endl;
       cout<<"------------------"<<endl;
   }
    return 0;
}
